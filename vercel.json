{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|svg|png|jpg|jpeg|gif|ico|woff|woff2|ttf|eot)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/www.qr-pro-generator.com/(.*)",
      "destination": "https://qr-pro-generator.com/$1",
      "permanent": true
    },
    {
      "source": "/www/(.*)",
      "destination": "https://qr-pro-generator.com/$1",
      "permanent": true
    },
    {
      "source": "/index.tsx",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/favicon.svg",
      "destination": "/favicon.svg"
    },
    {
      "source": "/sw.js",
      "destination": "/sw.js"
    },
    {
      "source": "/ads.txt",
      "destination": "/ads.txt"
    },
    {
      "source": "/wifi-qr-generator",
      "destination": "/index.html"
    },
    {
      "source": "/business-card-qr-generator",
      "destination": "/index.html"
    },
    {
      "source": "/restaurant-menu-qr",
      "destination": "/index.html"
    },
    {
      "source": "/bulk-qr-generator",
      "destination": "/index.html"
    },
    {
      "source": "/faq",
      "destination": "/index.html"
    },
    {
      "source": "/contact",
      "destination": "/index.html"
    },
    {
      "source": "/privacy",
      "destination": "/index.html"
    },
    {
      "source": "/terms",
      "destination": "/index.html"
    },
    {
      "source": "/free-qr-code-generator",
      "destination": "/index.html"
    },
    {
      "source": "/qr-code-maker",
      "destination": "/index.html"
    },
    {
      "source": "/custom-qr-codes",
      "destination": "/index.html"
    },
    {
      "source": "/wifi-qr-code-generator",
      "destination": "/index.html"
    },
    {
      "source": "/vcard-qr-generator",
      "destination": "/index.html"
    },
    {
      "source": "/qr-generator-free",
      "destination": "/index.html"
    },
    {
      "source": "/online-qr-code-generator",
      "destination": "/index.html"
    },
    {
      "source": "/qr-code-with-logo",
      "destination": "/index.html"
    },
    {
      "source": "/bulk-qr-code-generator",
      "destination": "/index.html"
    },
    {
      "source": "/restaurant-qr-code",
      "destination": "/index.html"
    },
    {
      "source": "/generator",
      "destination": "/index.html"
    },
    {
      "source": "/create",
      "destination": "/index.html"
    },
    {
      "source": "/make",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
